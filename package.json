{
  "name": "plugin-interceptor",
  "description": "plugin that records and mocks desired https/https requests and uses the recordings to measure command performance",
  "version": "1.0.0",
  "author": "Steve Hetzel @shetzel",
  "bugs": "https://github.com/shetzel/plugin-interceptor/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "tslib": "^1",
    "@salesforce/kit": "1.2.1",
    "sinon": "8.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/ts-sinon": "1.1.1",
    "@types/chai": "^4",
    "@types/debug": "0.0.30",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/fs-extra": "5.0.2",
    "@types/sinon": "4.3.1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/shetzel/plugin-interceptor",
  "keywords": [
    "oclif-plugin",
    "plugin-interceptor"
  ],
  "license": "MIT",
  "oclif": {
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "prerun": "./lib/hooks/interceptor.js"
    }
  },
  "repository": "shetzel/plugin-interceptor",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
